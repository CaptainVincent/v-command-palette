{
  "name": "v-command-palette",
  "type": "module",
  "version": "0.0.1",
  "packageManager": "pnpm@8.12.1",
  "description": "",
  "author": "Robert Soriano <sorianorobertc@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/wobsoriano/v-command-palette#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wobsoriano/v-command-palette.git"
  },
  "bugs": "https://github.com/wobsoriano/v-command-palette/issues",
  "keywords": [
    "vuetify",
    "vue",
    "command",
    "palette"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vue-tsc --noEmit && vite build --mode lib",
    "dev": "vite serve",
    "dev:build": "vite build",
    "dev:preview": "vite preview",
    "lint": "eslint .",
    "prepublishOnly": "pnpm build",
    "release": "bumpp && npm publish"
  },
  "peerDependencies": {
    "vue": "^3.2.0",
    "vuetify": "^3.3.0"
  },
  "dependencies": {
    "@vueuse/core": "^10.7.0",
    "fuse.js": "^7.0.0",
    "nanoid": "^5.0.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.4.6",
    "@mdi/font": "^7.3.67",
    "@types/node": "^18.19.3",
    "@vitejs/plugin-vue": "^4.5.2",
    "bumpp": "^9.2.1",
    "eslint": "^8.56.0",
    "roboto-fontface": "^0.10.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vite-plugin-dts": "^3.6.4",
    "vite-plugin-vuetify": "^2.0.1",
    "vue": "^3.3.12",
    "vue-tsc": "^1.8.25",
    "vuetify": "^3.4.7"
  }
}
